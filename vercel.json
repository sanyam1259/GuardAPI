{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/ui/(.*)",
        "dest": "app.py"
      }
    ],
    "scripts": {
         "build": "pip install -r requirements.txt"
     }
  }
  
